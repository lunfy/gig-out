<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/head') %>
  </head>

  <body>

    <header>
        <%- include('../partials/header') %> 
    </header>

    <main>
    <% gigs.forEach(item => { %>
        <div class="tile">
            <h2 class="title"><%= item.title %></h2>
            <p>Poster: <%= item.authorFname %> <%= item.authorLname %> </p>
            <br>
            <p><%= item.messageBody %></p>
            <br>
            <p class="p-date"><%= item.datePosted %></p>
        </div>
    <% }) %>
    </main>

  <footer>
    <!-- Insert footer partials here -->
  </footer>

  <!-- possible some script partials here -->
  <script> <%- include('../../public/scripts/navbar.js') %> </script>

  </body>
</html>