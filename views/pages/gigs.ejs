<!DOCTYPE html>
<html>
  <head>
    <%- include('../partials/head') %>
  </head>

  <body>

    <header>
        <%- include('../partials/header') %> 
    </header>

    <main>
      <div class="tile is-ancestor mt-5" id="tile-cont">
          <% gigs.forEach(item => { %>
          <div class="tile gig-tiles"><a href="/gigs/<%= item._id %>">
              <h2 class="title"> <%= item.title %></h2>
              <p class="subtitle">Poster: <%= item.authorFname %> <%= item.authorLname %> </p>
              <div class="content">
                  <%= item.messageBody %>
              </div>
              <p class="p-date"><%= item.datePosted %></p>
          </a></div>
          <% }) %>
      </div>
    </main>

  <footer>
    <!-- Insert footer partials here -->
  </footer>

  <!-- possible some script partials here -->
  <script> <%- include('../../public/scripts/navbar.js') %> </script>

  </body>
</html>